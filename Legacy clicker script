                                            function autoRebirth20m()
                                                spawn(function()
                                                    while getgenv().autoRebirth == true do
                                                local args = {
                                                    [1] = "Rebirth",
                                                    [2] = "20M Rebirths"
                                                }
                                                remotePate.rebirthRemote:FireServer(unpack(args))
                                                wait(0)
                                                    end 
                                                end)
                                                end

                                                function autoRebirth35m()
                                                    spawn(function()
                                                        while getgenv().autoRebirth == true do
                                                    local args = {
                                                        [1] = "Rebirth",
                                                        [2] = "35M Rebirths"
                                                    }
                                                    remotePate.rebirthRemote:FireServer(unpack(args))
                                                    wait(0)
                                                        end 
                                                    end)
                                                    end
