getgenv().autoTap = false;
getgenv().autoRebirth = false;
getgenv().buyEgg = false;

local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/AikaV3rm/UiLib/master/Lib.lua')))()

local w = library:CreateWindow("Auto Tap") -- Creates the window

local F = library:CreateWindow("Egg hatching --BETA--")

--local G = library:CreateWindow("Tp Coming soon")

local H = library:CreateWindow("Auto Rebirth")

local A = w:CreateFolder("Auto Tap") -- Creates the folder(U will put here your buttons,etc)

local b = F:CreateFolder("Egg hatching --BETA--")

--local C = G:CreateFolder("Tp Coming soon")

local D = H:CreateFolder("Auto Rebirth")
 
A:DestroyGui()

A:Toggle("Auto Tap",function(bool)
    getgenv().autoTap = bool
    print("Auto Tap is: ", bool)
    if bool then
        doTap();
    end
end)

