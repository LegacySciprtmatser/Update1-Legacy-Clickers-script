function autoRebirth1()
    spawn(function()
        while getgenv().autoRebirth == true do
    local args = {
        [1] = "Rebirth",
        [2] = "1 Rebirth"
    }
    remotePate.rebirthRemote:FireServer(unpack(args))
    wait(0)
        end 
    end)
    end

